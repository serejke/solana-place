{
  "name": "@serejke/solana-place-backend",
  "private": true,
  "version": "1.0.0",
  "description": "Backend for Solana Place",
  "author": "Sergei Patrikeev (@serejke)",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build": "webpack --mode production",
    "test": "mocha --recursive 'tests/*.test.ts'",
    "start": "node ./dist/server.js",
    "start:dev": "nodemon src/index.ts",
    "build:dev": "webpack --mode development",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "set -ex; eslint --ext .js,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix"
  },
  "testnetDefaultChannel": "edge",
  "dependencies": {
    "@godaddy/terminus": "^4.11.1",
    "@project-serum/anchor": "^0.25.0",
    "@sentry/node": "^7.1.1",
    "@sentry/tracing": "^7.0.0",
    "@solana/web3.js": "^1.43.7",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/node": "^17.0.41",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "bs58": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint": "^7.27.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "prettier": "^2.6.2",
    "terser": "^5.15.0",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0",
    "ws": "^8.7.0"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19"
  }
}
